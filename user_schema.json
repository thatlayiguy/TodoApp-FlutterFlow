{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "User",
  "type": "object",
  "properties": {
    "birthdate": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp representing the user's birthdate"
    },
    "created_time": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp when the user document was created"
    },
    "display_name": {
      "type": "string",
      "description": "Display name of the user"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "Email address of the user"
    },
    "photo_url": {
      "type": "string",
      "format": "uri",
      "description": "URL to the user's profile photo"
    },
    "uid": {
      "type": "string",
      "description": "Unique identifier for the user"
    }
  },
  "required": [
    "birthdate",
    "created_time",
    "display_name",
    "email",
    "photo_url",
    "uid"
  ]
}
